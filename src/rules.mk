# ######### DEBUG / CONSOLE #########
CONSOLE_ENABLE      = no      # Disable QMK console
COMMAND_ENABLE      = no      # Disable QMK command features

# ######### FEATURES #########
CAPS_WORD_ENABLE    = yes     # Enable Caps Word feature
LAYER_LOCK_ENABLE   = yes     # Enable Layer Lock
SPACE_CADET_ENABLE  = no      # Not using Space Cadet

# ######### RGB / LED #########
RGB_MATRIX_ENABLE        = yes     # Enable RGB Matrix
RGB_MATRIX_CUSTOM_KB     = yes     # Use your custom ledmap